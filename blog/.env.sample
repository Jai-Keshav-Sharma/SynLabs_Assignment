# API Keys
# GitHub personal access token for publishing blog posts
GITHUB_TOKEN=your_github_token_here

# OpenAI API key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# Groq API key for alternative LLM provider
GROQ_API_KEY=your_groq_api_key_here

# SerpAPI key for search results
SERPAPI_KEY=your_serpapi_key_here

# DataForSEO Credentials
# DataForSEO login email
DATAFORSEO_LOGIN=your_dataforseo_login_here

# DataForSEO password
DATAFORSEO_PASSWORD=your_dataforseo_password_here

# LLM Configuration
# LLM provider: "openai" or "groq" (default: "openai")
LLM_PROVIDER=openai

# LLM model to use (default: "gpt-4o-mini")
# For OpenAI: gpt-4o-mini, gpt-4o, gpt-3.5-turbo, etc.
# For Groq: llama-3.3-70b-versatile, etc.
LLM_MODEL=gpt-4o-mini
